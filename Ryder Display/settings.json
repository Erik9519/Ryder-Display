        "ui_notify": true,
        "hyperion_effect": "Notify Discord"
      }
    },
    "power_plans": [
      "Power saver",
      "Balanced",
      "High performance"
    ],
    "audio_presets": [
      {
        "playbackDevice": "Speakers",
        "playbackDeviceCommunication": "Speakers",
        "recordingDevice": "Camera Microphone"
